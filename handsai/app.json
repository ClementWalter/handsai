{
  "expo": {
    "name": "handsai",
    "slug": "handsai",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "primaryColor": "#ffffff",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.handsai.clementwalter",
      "buildNumber": "1.0.0",
      "supportsTablet": false,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to send images to a remote server for classification."
      }
    },
    "android": {
      "package": "com.handsai.clementwalter",
      "versionCode": 1,
      "permissions" : ["CAMERA"]
    },
    "description": ""
  }
}
